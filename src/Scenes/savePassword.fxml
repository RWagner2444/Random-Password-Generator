<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SavePasswordController">
   <children>
      <Label layoutX="98.0" layoutY="50.0" prefHeight="50.0" prefWidth="200.0" text="Save Password" textAlignment="CENTER">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Label layoutX="76.0" layoutY="203.0" prefHeight="19.0" prefWidth="58.0" text="Username" />
      <Label layoutX="75.0" layoutY="164.0" prefHeight="17.0" prefWidth="60.0" text="Website" />
      <Label layoutX="76.0" layoutY="122.0" prefHeight="17.0" prefWidth="58.0" text="File Name" />
      <TextField fx:id="fileNameTF" layoutX="149.0" layoutY="118.0" prefHeight="12.0" prefWidth="105.0" />
      <TextField fx:id="websiteTF" layoutX="149.0" layoutY="160.0" prefHeight="24.0" prefWidth="104.0" />
      <TextField fx:id="userNameTF" layoutX="148.0" layoutY="199.0" prefHeight="28.0" prefWidth="105.0" />
      <Button fx:id="saveButton" layoutX="168.0" layoutY="246.0" mnemonicParsing="false" onAction="#savePass" prefHeight="30.0" prefWidth="60.0" text="Save">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button fx:id="backButton" layoutX="25.0" layoutY="24.0" mnemonicParsing="false" onAction="#back" prefHeight="22.0" prefWidth="50.0" text="Back" />
   </children>
</Pane>
